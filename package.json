{
  "name": "react-beautiful-schedule",
  "version": "1.0.0",
  "description": "A simple React weekly event schedule component",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "files": [
    "build"
  ],
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmedalatawi/react-beautiful-schedule"
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "build-webpack": "webpack --config webpack.config.js",
    "dev": "webpack-dev-server --mode development --open --hot",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,demo,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "keywords": [
    "react",
    "typescript",
    "calendar",
    "schedule",
    "weekly",
    "days",
    "events"
  ],
  "author": "Ahmed Alatawi",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
